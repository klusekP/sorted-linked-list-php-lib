{
    "$schema": "https://raw.githubusercontent.com/infection/infection/0.29.0/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "infection.log",
        "summary": "infection-summary.log"
    },
    "mutators": {
        "@default": true
    },
    "minMsi": 94,
    "minCoveredMsi": 94,
    "timeout": 10,
    "testFramework": "phpunit",
    "testFrameworkOptions": "--stop-on-failure"
}
